<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
		<!--  <langChildren>1</langChildren>!-->
	</meta>
	<ROOT>
		<type>array</type>
		<el>
			<name>
				<TCEforms>
					<label>LLL:EXT:powermail/locallang_db.xml:tx_powermail_forms.xml.name</label>
					<config>
						<type>input</type>
						<size>35</size>
						<max>20</max>
						<eval>required,trim,lower,alphanum_x,nospace</eval>
					</config>
				</TCEforms>
			</name>
			<value>
				<TCEforms>
					<exclude>1</exclude>
					<label>LLL:EXT:powermail/locallang_db.xml:tx_powermail_forms.xml.value</label>
					<config>
						<type>group</type>
						<internal_type>db</internal_type>
						<allowed>tt_content</allowed>
						<size>1</size>
						<maxitems>1</maxitems>
						<minitems>0</minitems>
						<show_thumbs>1</show_thumbs>
					</config>
				</TCEforms>
			</value>
			<send>
				<TCEforms>
					<label>LLL:EXT:powermail/locallang_db.xml:tx_powermail_forms.xml.send</label>
					<config>
						<type>check</type>
					</config>
				</TCEforms>
			</send>
			<example>
				<TCEforms>
					<label>LLL:EXT:powermail/locallang_db.xml:tx_powermail_forms.xml.example</label>
					<config>
						<type>user</type>
						<userFunc>tx_powermail_tx_powermail_example->main</userFunc>
						<field>content</field>
					</config>
				</TCEforms>
			</example>
		</el>
	</ROOT>
</T3DataStructure>